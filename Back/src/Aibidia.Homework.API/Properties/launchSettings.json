{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Aibidia.Homework": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "launchUrl": "swagger",
      "applicationUrl": "https://localhost:7013;http://localhost:5013",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger/index.html",
      "applicationUrl": "http://host.docker.internal:7013;http://host.docker.internal:5013",
      "useSSL": false,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Docker",
        "ASPNETCORE_URLS": "http://+:443;http://+:80"
      }
    }
  }
}
